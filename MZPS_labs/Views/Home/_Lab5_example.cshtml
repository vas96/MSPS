        <hr />
        <table class="table table-hover">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">ftp</th>
                    <th scope="col">Ncp</th>
                    <th scope="col">Cp</th>
                    <th scope="col">fTRAM</th>
                    <th scope="col">Vram</th>
                    <th scope="col">Vhdd</th>
                    <th scope="col">Shdd</th>
