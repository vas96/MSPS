                    <th scope="col">Cnet</th>
                    <th scope="col">Rp</th>
                    <th scope="col">Vpr</th>
                    <th scope="col">Re</th>
                    <th scope="col">Vpram</th>
                    <th scope="col">Ccos</th>
                    <th scope="col">Ncos</th>
                    <th scope="col">Ntos</th>
                    <th scope="col">Nuos</th>
                    <th scope="col">Tos</th>
                    <th scope="col">Cdb</th>
                    <th scope="col">Vdb</th>
                    <th scope="col">Vdbt</th>
                    <th scope="col">Vdbcb</th>
                    <th scope="col">Vdbdt</th>
                    <th scope="col">Tdb</th>
                    <th scope="col">Ce</th>
                    <th scope="col">Nef</th>
                    <th scope="col">Ned</th>
                    <th scope="col">Ved</th>
                    <th scope="col">Crg</th>
                    <th scope="col">Vrgin</th>
                    <th scope="col">Nrgc</th>
                    <th scope="col">Nrgfr</th>
                    <th scope="col">Nrggf</th>
                    <th scope="col">Nrgdb</th>
                    <th scope="col">Trg</th>
                    <th scope="col">Nnm</th>
                    <th scope="col">Pnm</th>
                    <th scope="col">Tnm</th>
                    <th scope="col">Tpd</th>
                    <th scope="col">Tpid</th>
                    <th scope="col">Tarr</th>
                </tr>
            </thead>
            <tbody>
                @{int num = 1;}
                @foreach (List<double> subList in Model[0])
                {
                    <tr>
                        <td scope="row">@num</td>;
                        @{++num;}

                        @foreach (double item in subList)
                        {

                            <td>@item</td>;

                        }
                    </tr>
                }

            </tbody>
        </table>

    </div>
</div>



